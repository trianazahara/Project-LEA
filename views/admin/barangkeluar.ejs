<%- include("../sidebar.ejs") %>
<div class="w-full pt-10 px-4 sm:px-6 md:px-8 lg:ps-72">
  <h3 class="font-bold md:text-xl md:leading-tight">Barang Keluar</h3>

  <div class="max-w-[85rem] mx-auto py-10 px-4 sm:px-6 lg:px-8">
    <div class="flex flex-col">
      <div class="-m-1.5 overflow-x-auto">
        <div class="p-1.5 min-w-full inline-block align-middle">
          <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">
            <div class="px-6 py-4 grid gap-3 md:flex md:justify-between md:items-center border-b border-gray-200">
              <div class="sm:col-span-1">
                <label for="hs-as-table-product-review-search" class="sr-only">Search</label>
                <div class="relative">
                  <input
                    type="text"
                    id="hs-as-table-product-review-search"
                    name="hs-as-table-product-review-search"
                    class="py-2 px-3 ps-11 block w-full border border-gray-200 rounded-lg text-sm focus:border-blue-500 focus:ring-blue-500 disabled:opacity-50 disabled:pointer-events-none"
                    placeholder="Search"
                  />
                  <div class="absolute inset-y-0 start-0 flex items-center pointer-events-none ps-4">
                    <svg
                      class="flex-shrink-0 size-4 text-gray-400"
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 24 24"
                      fill="none"
                      stroke="currentColor"
                      stroke-width="2"
                      stroke-linecap="round"
                      stroke-linejoin="round"
                    >
                      <circle cx="11" cy="11" r="8" />
                      <path d="m21 21-4.3-4.3" />
                    </svg>
                  </div>
                </div>
              </div>
              <!-- Tombol Lihat dan Tambah -->
              <div class="flex items-center">
                <button
                  type="button"
                  class="ml-2 py-2 px-4 bg-blue-500 text-white rounded-lg hover:bg-blue-600 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2"
                >
                  Lihat
                </button>
                <button
                  type="button"
                  class="ml-2 py-2 px-4 bg-green-500 text-white rounded-lg hover:bg-green-600 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-offset-2"
                >
                  + Tambah
                </button>
              </div>
            </div>

            <!-- Table -->
            <table class="min-w-full divide-y divide-gray-200 text-sm">
              <thead class="bg-gray-50">
                <tr>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Nama Barang</span>
                  </th>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Kategori</span>
                  </th>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Gambar</span>
                  </th>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Tanggal Masuk</span>
                  </th>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Supplier</span>
                  </th>
                  <th scope="col" class="px-4 py-2 text-start">
                    <span class="font-semibold text-gray-800">Aksi</span>
                  </th>
                </tr>
              </thead>
              <!-- Isi tabel di sini -->
              <tbody class="bg-white divide-y divide-gray-200">
                <!-- Contoh baris data -->
                <tr>
                  <td class="px-4 py-2">Nama Barang 1</td>
                  <td class="px-4 py-2">Kategori A</td>
                  <td class="px-4 py-2"> <img src="gambar" alt="gambar"> </td>
                  <td class="px-4 py-2">2024-07-07</td>
                  <td class="px-4 py-2">Supplier X</td>
                  <td class="px-4 py-2">
                    <button
                      type="button"
                      class="py-1 px-2 bg-blue-500 text-white rounded-lg hover:bg-blue-600 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2"
                    >
                      Edit
                    </button>
                    <button
                      type="button"
                      class="ml-2 py-1 px-2 bg-red-500 text-white rounded-lg hover:bg-red-600 focus:outline-none focus:ring-2 focus:ring-red-500 focus:ring-offset-2"
                    >
                      Hapus
                    </button>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="/preline.js"></script>
